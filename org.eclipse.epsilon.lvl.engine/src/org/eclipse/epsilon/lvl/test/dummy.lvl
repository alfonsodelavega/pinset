dataset Dummy01_As over a : A {
  properties [x, en]
  @silent
  column twiceX : a.x * 2
}

dataset Dummy02_AsAndPartialBs over a : A {
  properties [x, en]
  reference b[y]
}

dataset Dummy03_AsAndFullBs over a : A {
  properties [x, en]
  reference b
}

dataset Dummy04_AsAndNumberOfDs over a : A {
  properties [x]
  column numberOfDs : a.ds.size()
}

dataset Dummy05_filteredAs over a : A {
  guard : a.x > 5
  properties [x]
  column numberOfDs : a.ds.size()
}

dataset Dummy06_grid over a : A {
  properties [x]
  grid {
    keys   : 1.to(5)
    header : "x_plus_" + key
    body   : a.x + key
  }
}

dataset Dummy07_features over a : A {
  properties [x]
  features [y, z] {
    from : a.b
    header : "b_" + feature
  }
}

dataset Dummy08_from over a : A
                     from : A.all.select(a | a.x > 5) {
  properties [x]
  features [y, z] {
    from : a.b
    header : "b_" + feature
  }
}

dataset Dummy09_from over number
                     from : 1.to(5) {
  column number : number
  grid {
    keys : 2.to(5)
    header : "times_" + key
    body : number * key
  }
}

dataset Dummy10a_silentColumn over a : A {
  properties [x]
  @silent
  column b_y : a.b.y // there is an A element that does not have a B
}

@silent
dataset Dummy10b_silentColumn over a : A {
  properties [x]
  column b_y : a.b.y // there is an A element that does not have a B
}

@silent
dataset Dummy11a_silentGrid over a : A {
  grid {
    keys : 2.to(5)
    header : "times_" + key
    body : a.getNonExistentValue(key) // fails if annotation removed
  }
}

dataset Dummy11b_silentGrid over a : A {
  @silent
  grid {
    keys : 2.to(5)
    header : "times_" + key
    body : a.getNonExistentValue(key) // fails if annotation removed
  }
}

dataset Dummy12a_from over a : A
                     from : A.all.select(a | a.x > 5) {
  properties [x]
  @silent
  features [y, z] {
    from : a.getNonExistentValue() // fails if annotation removed
    header : "b_" + feature
  }
}

@silent
dataset Dummy12b_from over a : A
                     from : A.all.select(a | a.x > 5) {
  properties [x]
  features [y, z] {
    from : a.getNonExistentValue() // fails if annotation removed
    header : "b_" + feature
  }
}

dataset Dummy13_gridReturnBlocks over a : A {
  grid {
    keys {
      return 1.to(5);
    }
    header {
      return key;
    }
    body {
      return key * 10;
    }
  }
}
